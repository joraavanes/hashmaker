{
    "env": {
        "browser": false,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    // "extends": [
    //     "airbnb-base"
    // ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-undef":"error",
        "no-const-assign":"error",
        "no-param-reassign":"error",
        "no-unused-vars": "warn",
        "no-dupe-keys":"error",
        "no-empty": ["warn", {
            "allowEmpyCatch": true
        }]
    }
}
